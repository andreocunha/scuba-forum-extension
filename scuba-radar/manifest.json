{
    "name": "Scuba Radar",
    "description": "Extens√£o do Scuba Team",
    "version": "1.2",
    "manifest_version": 2,
    "icons": {
        "16": "icon.png",
        "32": "icon.png",
        "64": "icon.png",
        "128": "icon.png",
        "256": "icon.png"
    },
    "browser_action": {
        "default_title": "Scuba Radar",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "content_scripts":[
        {
          "matches": ["https://cursos.alura.com.br/forum/*"],
          "js": ["manipulateDom.js"],
          "css": ["token.css"]
        }
    ],
    "background": {
        "scripts": [
            "socket.io.js",
            "background.js"
        ]
    },
    "permissions": [
        "tabs",
        "storage"
    ]
}
